{
	"name": "@mhazaa/analytics-engine",
	"version": "1.0.13",
	"description": "Simple analytics tracker for Express and MongoDB",
	"main": "./build/cjs/src/index.js",
	"module": "./build/esm/src/index.js",
	"exports": {
		".": {
			"require": "./build/cjs/src/index.js",
			"import": "./build/esm/src/index.js"
		},
		"./client": {
			"require": "./build/cjs/src/client/index.js",
			"import": "./build/esm/src/client/index.js"
		}
	},
	"scripts": {
		"build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
		"prepublishOnly": "npm run build"
	},
	"keywords": [],
	"author": "Magdi Hazaa",
	"license": "ISC",
	"dependencies": {
		"@mhazaa/mongo-controller": "^1.0.12",
		"@types/express": "^4.17.13",
		"@types/uniqid": "^5.3.2",
		"uniqid": "^5.4.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"eslint": "^8.5.0",
		"typescript": "^4.5.4"
	}
}
